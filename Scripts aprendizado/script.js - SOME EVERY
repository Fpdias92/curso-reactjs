// SOME
// Verifica se tem pelo menos um item na array
let nomes = ['Felipe', 'Augusto'];
console.log(nomes.some(nomes => nomes === 'Felipe'));

// EVERY
// Verifica se todos os itens tem idade acima de 18 anos
let nomes2 = [
    {nome: 'Felipe', idade: 30},
    {nome: 'Augusto', idade: 56}
];

console.log(nomes2.every( nomes2 => nomes2.idade >=18));